{
  "compilerOptions": {
    "outDir": "./dist/",
    // "sourceMap": true,
    "noImplicitAny": true,
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    /*"noEmit": true,*/
    "jsx": "react",
    "types": ["jest", "liff-type"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dev_server.js"]
}
